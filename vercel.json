{
  "buildCommand": "npm run build",
  "outputDirectory": "dist", 
  "framework": "vite", 
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ], 
  "rewrites": [
    { "source": "/resume.pdf", "destination": "/assets/resume.pdf" },
    { "source": "/dance_cv.pdf", "destination": "/assets/dance_cv.pdf" }, 
    { "source": "/(.*)", "destination": "/"  }
  ]
}
