{
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ], 
  "rewrites": [
    { "source": "/resume.pdf", "destination": "/assets/resume.pdf" },
    { "source": "/dance_cv.pdf", "destination": "/assets/dance_cv.pdf" }, 
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
